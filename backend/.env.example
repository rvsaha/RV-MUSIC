# .env.example for backend (Render)

# JWT secret for signing tokens
JWT_SECRET=your-very-strong-secret

# OAuth credentials (replace with your real values)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
FB_CLIENT_ID=your-facebook-client-id
FB_CLIENT_SECRET=your-facebook-client-secret
TWITTER_CONSUMER_KEY=your-twitter-consumer-key
TWITTER_CONSUMER_SECRET=your-twitter-consumer-secret

# CORS allowed origin (your Netlify frontend URL)
CORS_ORIGIN=https://your-site.netlify.app
